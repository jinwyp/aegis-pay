<div class="compactUpload">
  <div action='post' id="signCompact">
      <div class="row fileupload-buttonbar">
        <span class="btn btn-success fileinput-button">
            <i class="glyphicon glyphicon-plus"></i>
            <span>Add files...</span>
            <input id='fileupload' data-url='/api/upload-file' type="file" name="files">
        </span>
      </div>
      <div id="files"></div>
      <input type="hidden" name="orderid" value="<%= orderid %>"/>
      <input type="hidden" name="action" value="post">
      <input type="button" class='submit' value="确认提交" />
      <input type="button" class='cancel' value="取消" data-toggle='modal' data-target='#cancelSignModel'/>
  </div>
  <div id="cancelSignModel" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
      <h3>Modal header</h3>
    </div>
    <div class="modal-body">
      <p>确定取消签订合同？</p>
    </div>
    <div class="modal-footer">
      <button class="btn" data-dismiss="modal" aria-hidden="true">取消</button>
      <a href='/orders' class="btn btn-primary">确认</a>
    </div>
  </div>
</div>
