<%- include ../global/includehead/doctype %>
<head>
    <%- include ../global/includehead/head %>
    <link rel="stylesheet" href="/dist/styles/financialHome.css">
</head>

<body>

<%- include ../global/header/financialFirstHeader %>
<%- include ../global/header/financialSecondHeader %>

<div id="financial-body">
    <div class="financial-main">

        <div class="row financial-details">
            <form class="form-inline">
                <div class="form-row">
                    <div class="form-group form-search">
                        <label class="col-sm-3 control-label" for="orderCategory">订单分类:</label>

                        <select class="form-control inputselect" id="orderCategory" >
                            <option>全部</option>
                            <option>2</option>
                        </select>
                    </div>

                    <div class="form-group form-search">
                        <label class="col-sm-3 control-label" for="orderCategory">交易时间:</label>

                        <select class="form-control inputselect" id="orderCategory" >
                            <option>1</option>
                            <option>2</option>
                        </select>
                    </div>

                    <div class="form-group form-search">
                        <label class="col-sm-3 control-label" for="orderCategory">交易状态:</label>

                        <select class="form-control inputselect" id="orderCategory" >
                            <option>全部</option>
                            <option>2</option>
                        </select>
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group form-search">
                        <label class="col-sm-3 control-label" for="orderCategory">交易状态:</label>

                        <select class="form-control inputselect" id="orderCategory" >
                            <option>全部</option>
                            <option>2</option>
                        </select>
                    </div>

                    <div class="form-group form-search">
                        <label class="sr-only" for="exampleInputEmail3">交易编号</label>
                        <input type="text" class="form-control inputselect inputbox" id="exampleInputName2" placeholder="请输入编号号码">
                    </div>

                    <button type="submit" class="iBtn iBtn_sm iBtn_yes">查询</button>
                </div>



            </form>

            <div class="row table-box">
                <div class="list-title">
                    <span class="record-title">易煤网资金账户号:</span>
                    <span class="record-no">1234567890</span>
                </div>
                <div class="financial-center">
                    <div class="record-div">
                        <table class="table financeDetailTable">
                            <tr>
                                <th >交易日期</th>
                                <th >交易流水号</th>
                                <th >金额</th>
                                <th >账户余额</th>
                                <th >交易类型</th>
                                <th >对方账号</th>
                                <th >对方账号名称</th>
                                <th >对方开户行</th>
                                <th >操作</th>
                            </tr>
                            <tr>
                                <td >2016-05-12</td>
                                <td >123456789012345678</td>
                                <td ><%= formatMoney(order.totalMoney,2) %></td>
                                <td >交易成功</td>
                                <td >交易成功</td>
                                <td >交易成功</td>
                                <td ><span class="iBtn iBtn_sm iBtn_no">付款</span></td>
                            </tr>
                        </table>

                        <nav>
                            <ul class="pagination">
                                <li>
                                    <a href="#" aria-label="Previous">
                                        <span aria-hidden="true">&laquo;</span>
                                    </a>
                                </li>
                                <li><a href="#">1</a></li>
                                <li><a href="#">2</a></li>
                                <li><a href="#">3</a></li>
                                <li><a href="#">4</a></li>
                                <li><a href="#">5</a></li>
                                <li>
                                    <a href="#" aria-label="Next">
                                        <span aria-hidden="true">&raquo;</span>
                                    </a>
                                </li>
                            </ul>
                        </nav>
                    </div>


                </div>
            </div>

        </div>


    </div>
</div>

<%- include ../global/footer/footer %>

<%- include ../global/includehead/bottomscript %>

<script src="/dist/scripts/header/financialHeader.js"></script>
<script src="/dist/scripts/wealth/financialHome.js"></script>

</body>
</html>



