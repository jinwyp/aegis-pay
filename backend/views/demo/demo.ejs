<%- include ../global/includehead/doctype %>
<head>
    <%- include ../global/includehead/head %>
    <link rel="stylesheet" href="/dist/styles/demo.css">
</head>

<body>
<%- include ../global/header/header %>

<!-- 按钮颜色&大小 -->
<table class="btnBoxList">
    <tbody>
    <tr>
        <th width="130">样式尺寸</th>
        <th>编辑, 更改</th>
        <th>确认, 提交</th>
        <th>退回, 否定</th>
        <th>取消, 返回</th>
        <th>禁用, disabled</th>
        <th>只读, readonly</th>
    </tr>
    <tr>
        <th>按钮_大:  140*40</th>
        <td><input type="button" class="iBtn iBtn_edit" value="修改"/></td>
        <td><input type="button" class="iBtn iBtn_yes" value="确认"/></td>
        <td><input type="button" class="iBtn iBtn_no" value="退回"/></td>
        <td><input type="button" class="iBtn iBtn_def" value="取消"/></td>
        <td><input type="button" class="iBtn iBtn_yes" value="不可用" disabled/></td>
        <td><input type="button" class="iBtn iBtn_yes" value="只读" readonly/></td>
    </tr>
    <tr>
        <th>按钮_中:  96*32</th>
        <td><input type="button" class="iBtn iBtn_sm iBtn_edit" value="修改"/></td>
        <td><input type="button" class="iBtn iBtn_sm iBtn_yes" value="确认"/></td>
        <td><input type="button" class="iBtn iBtn_sm iBtn_no" value="退回"/></td>
        <td><input type="button" class="iBtn iBtn_sm iBtn_def" value="取消"/></td>
        <td><input type="button" class="iBtn iBtn_sm iBtn_yes" value="不可用" disabled/></td>
        <td><input type="button" class="iBtn iBtn_sm iBtn_yes" value="只读" readonly/></td>
    </tr>
    <tr>
        <th>按钮_小:  60*24</th>
        <td><input type="button" class="iBtn iBtn_xs iBtn_edit" value="修改"/></td>
        <td><input type="button" class="iBtn iBtn_xs iBtn_yes" value="确认"/></td>
        <td><input type="button" class="iBtn iBtn_xs iBtn_no" value="退回"/></td>
        <td><input type="button" class="iBtn iBtn_xs iBtn_def" value="取消"/></td>
        <td><input type="button" class="iBtn iBtn_xs iBtn_yes" value="不可用" disabled/></td>
        <td><input type="button" class="iBtn iBtn_xs iBtn_yes" value="只读" readonly/></td>
    </tr>


    <tr>
        <th>文本_大:  88% * 40</th>
        <td><input type="text" class="iIpt" value="" placeholder="正常状态" /></td>
        <td><input type="text" class="iIpt" value="输入状态" placeholder="请输入" /></td>
        <td><input type="text" class="iIpt iIpt_err" value="输入错误" placeholder="" /></td>
        <td></td>
        <td><input type="text" class="iIpt" value="" placeholder="禁用状态" disabled /></td>
        <td><input type="text" class="iIpt" value="" placeholder="只读状态" readonly /></td>
    </tr>
    <tr>
        <th>文本_中:88% * 32</th>
        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="请输入" /></td>
        <td><input type="text" class="iIpt iIpt_sm" value="输入状态" placeholder="请输入" /></td>
        <td><input type="text" class="iIpt iIpt_sm iIpt_err" value="输入错误" placeholder="" /></td>
        <td></td>
        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="禁用状态" disabled /></td>
        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="只读状态" readonly /></td>
    </tr>
    <tr>
        <th>文本_小:  88% * 24</th>
        <td><input type="text" class="iIpt iIpt_xs" value="" placeholder="正常状态" /></td>
        <td><input type="text" class="iIpt iIpt_xs" value="输入状态" placeholder="请输入" /></td>
        <td><input type="text" class="iIpt iIpt_xs iIpt_err" value="输入错误" placeholder="" /></td>
        <td></td>
        <td><input type="text" class="iIpt iIpt_xs" value="" placeholder="禁用状态" disabled /></td>
        <td><input type="text" class="iIpt iIpt_xs" value="" placeholder="只读状态" readonly /></td>
    </tr>
    </tbody>
</table>
<p>&nbsp;</p>


