<p>&nbsp;</p>
<div class="icnList">
    <input type="button" class="btn btn-sm iBtn_no" value="模态框:提议" data-toggle="modal" data-target="#reasonsReturnModal" />
    <input type="button" class="btn btn-sm iBtn_no" value="模态框:提示" data-toggle="modal" data-target="#operationInfoModal" />

    <a href="#" class="btn btn-sm btn-info" id="msg_err">错误:提示框</a>
    <a href="#" class="btn btn-sm btn-info" id="msg_alt">警告:提示框</a>
    <a href="#" class="btn btn-sm btn-info" id="msg_info">信息:提示框</a>
    <a href="#" class="btn btn-sm btn-info" id="msg_done">完成:提示框</a>


    <span class="iBtn iBtn_down">
        <i class="iBtn_down_icn"></i>下载附件
    </span>
    <!-- 上传 -->
    <span class="iBtn iBtn_up">
        <i class="iBtn_up_icn"></i>点击上传
        <input id="upFile" class="iptFile" data-url="/api/upload-file" type="file" name="files">
    </span>
    <div class="fileList" style="display: inline-block">
        <p class="fileLab">
            <span class="fileLab_name">AAAAAAAAAAAAAA.JPG</span>
            <span class="fileLab_del" data-id=""></span>
        </p>
    </div>
</div>


<div class="containerWrap">
    <div class="bodyWrap">
        <!-- 状态条 -->
        <% include ../global/order/statusBar %>
        <p>&nbsp;</p>


        <!-- 退回原因提示框 板块 -->
        <%- include ../Plugins/reasonsReturnPlate %>
        <p>&nbsp;</p>


        <!-- 结算单信息 板块-->
        <%- include ../Plugins/settlementInfoPlate %>

    </div>
</div>





















<!--测试列表 -->
<div class="demoList fl">
    <table id="listBox">
        <thead>
        <tr>
            <th>序号</th>
            <th>城市</th>
            <th>描述</th>
        </tr>
        </thead>
        <tbody>

        <% listData.forEach(function(city){ %>
        <tr>
            <td><%= city.id%></td>
            <td><%= city.name%></td>
            <td><%= city.desc%></td>
        </tr>
        <% }) %>
        </tbody>
    </table>
</div>


<!-- 结算单退回原因: 模态框-->
<div class="modal fade" id="reasonsReturnModal" role="dialog">
    <div class="modal-dialog modal-pl" role="document">
        <div class="modal-content mt220 ">
            <div class="modal-header">
                <span class="close" data-dismiss="modal" aria-label="Close"></span>
                <h4 class="modal-title">提示</h4>
            </div>
            <div class="modal-body">
                <div class="pad_15">
                    <div class="txtAreaBox">
                        <div class="dockDown">您还可以输入<span class="limitNum">200</span>个字</div>
                        <textarea class="txtArea .returnText" name="remarks" rows="10" maxlength="200" placeholder="请简要填写退回原因，限200个字"></textarea>
                    </div>
                </div>
            </div>
            <p class="txt_c">
                <input type="button" class="iBtn iBtn_def" value="取消" data-dismiss="modal" />
                <input type="button" class="iBtn iBtn_yes ml20" value="确认" />
                <br/><br/><br/>
            </p>
            <!--<div class="modal-footer txt_c"></div>-->
        </div><!-- /.modal-content -->
    </div>
</div>


<!-- 操作提示信息: 模态框-->
<div class="modal fade" id="operationInfoModal" role="dialog">
    <div class="modal-dialog modal-sm" role="document">
        <div class="modal-content mt150 ">
            <div class="modal-header">
                <span class="close" data-dismiss="modal" aria-label="Close"></span>
                <h4 class="modal-title">提示</h4>
            </div>
            <div class="modal-body">
                <span class="icn_successMax option_info_icn"></span>
                <p class="option_info">结算单已重新提交到买家确认！</p>
                <p class="option_warn"><span class="bright">5S</span> 后自动关闭</p>
                <p>&nbsp;</p>
            </div>
            <!--<div class="modal-footer txt_c"></div>-->
        </div><!-- /.modal-content -->
    </div>
</div>








