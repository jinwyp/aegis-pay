<!DOCTYPE html>
<html >
<head>
    <meta charset="UTF-8">
    <title>Title</title>

    <style>

        .financeDetailTable { width: 820px; margin-bottom: 0px; border-collapse:collapse;}

        .financeDetailTable th { color: #AEAEAE; background-color: #F5F5F5; text-align: center; border: 1px;
            padding: 8px;
            line-height: 1.42857143;
            vertical-align: top;
            font-weight: normal;
        }

        .financeDetailTable tr { border-bottom: 1px dotted #e8e8e8; }

        .financeDetailTable tr:hover { background-color: #effafd; }

        .financeDetailTable td { padding: 20px 1px; text-align: center; font-size: 14px; color: #323333; vertical-align: middle; border-top: 0px dotted #e8e8e8; }


    </style>
</head>
<body>
<div >
    <div class="record-div">
        <table class="table financeDetailTable">
            <tr>
                <th >交易日期</th>
                <th >交易流水号</th>
                <th >金额</th>
                <th >账户余额</th>
                <th >摘要</th>
                <th >对方账号</th>
                <th >对方账号名称</th>
            </tr>

            <% orderList.forEach(function(order){ %>
            <tr>
                <td><%= order.createDate %></td>
                <td><%= order.transactionNO %></td>
                <td><%= helper.formatMoney(order.money) %> </td>
                <td><%= helper.formatMoney(order.balanceMoney) %></td>
                <td><%= order.type %> 订单号:<%= order.orderId%></td>
                <td><%= order.otherFundAccount %></td>
                <td><%= order.otherCompanyName %></td>
            </tr>
            <% }); %>

        </table>
    </div>


</div>
</body>
</html>

