<div id="paymentHeader">
    <div class="headWrap">
            <div class="wrap1000">
                <ul class="menuList">
                    <li class="noborder"><span class="icon-phone"></span><span>移动客户端</span></li>
                </ul>

                <ul class="menuList rightMenuList">
                    <li>欢迎来到易煤网<% if(user && user.id){ %>，<a href="javascript:void(0)"><%= user.nickname %></a><% } %></li>

                    <!--登录状态-->
                    <% if(user && user.id){ %>
                    <li>职务：<%= user.nickname %></li>
                    <li><a href="<%= config.passport.member %>/logout?gotoURL=<%= currentLocation %>&from=<%= config.domain %>">退出</a></li>
