<div id="subHeader">
    <div class="subHeaderWrap">
        <div class="subHeader1000">
            <a href="<%= config.site_page%>"><img src="<%= staticPath %>/images/header/yimeiLogo.png" alt=""/></a>
            <span class="subHeaderTit"><%= headerTit %></span>
            <div class="tab-cards">
                <ul class="firstTab">
                    <li>
                        <a class="a-option <% if(tabObj.firstTab==1){ %>selected<% } %>" href="/wealth/financialHome" data-value="1">首页</a>
                    </li>
                    <li><a class="a-option <% if(tabObj.firstTab==2){ %>selected<% } %>" href="/wealth/addAccount" data-value="2">账户管理</a></li>
                    <li><a class="a-option <% if(tabObj.firstTab==3){ %>selected<% } %>" href="/wealth/financialTransaction" data-value="3">交易管理</a></li>
                    <li><a class="a-option <% if(tabObj.firstTab==4){ %>selected<% } %>" href="/wealth/financialSettlement" data-value="4">结算管理</a></li>
                    <li><a class="a-option <% if(tabObj.firstTab==5){ %>selected<% } %>" href="/wealth/financialContract" data-value="5">合同管理</a></li>
                </ul>
                <span class="arrow-blue tab-step-<%= tabObj.firstTab %>"></span>
            </div>
        </div>

        <div class="second-tab">
            <ul class="secondTab <% if(tabObj.firstTab!=2){ %>hidden-ul<% } %>">
                <li class="<% if(tabObj.secondTab==1){ %>selected<% } %>"><a href="/wealth/addAccount" data-value="1">账户通</a></li>
                <li class="<% if(tabObj.secondTab==2){ %>selected<% } %>"><a href="/wealth/financialDetails" data-value="2">收支明细</a></li>
                <li class="<% if(tabObj.secondTab==3){ %>selected<% } %>"><a href="/account/accountSetting" data-value="3">账户设置</a></li>
            </ul>
            <ul class="secondTab <% if(tabObj.firstTab!=4){ %>hidden-ul<% } %>">
                <li class="<% if(tabObj.secondTab==1){ %>selected<% } %>"><a href="/wealth/financialSettlement" data-value="1">结算</a></li>
                <li class="<% if(tabObj.secondTab==2){ %>selected<% } %>"><a href="/settlement/billCenter" data-value="2">发票中心</a></li>
            </ul>
        </div>
    </div>
</div>
