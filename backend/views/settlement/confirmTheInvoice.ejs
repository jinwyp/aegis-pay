<%- include ../global/includehead/doctype %>
<head>
    <%- include ../global/includehead/head %>
    <link rel="stylesheet" href="/dist/styles/confirmTheInvoice.css">
</head>

<body>

<%- include ../global/header/header %>
<%- include ../global/header/subHeader %>


<!-- 以下.内容部分 -->
<div class="containerWrap">
    <div class="bodyWrap">

        <!-- 添加开票信息 板块 -->
        <div id="addInvoicePlate" class="plateBox">
            <div class="plateHead borSolid">
                <span class="prefix"></span>
                <h3 class="headLeft fl"><%= subTitle %></h3>
                <!--<p class="headContent">居中标题</p>-->
                <p class="headRight">
                    <span class="doubtInlet">
                        <s class="require">*</s> 为必填项目
                    </span>
                </p>
            </div>
            <div class="plateMain">
                <table class="table">
                    <tbody>
                    <tr>
                        <td class="txt_r" width="358">公司名称：</td>
                        <td width="340"><span class="s">和略电子商务有限公司</span></td>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <td class="txt_r"><s class="require">*</s>公司地址：</td>
                        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="正常状态"></td>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <td class="txt_r"><s class="require">*</s>公司电话：</td>
                        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="正常状态"></td>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <td class="txt_r"><s class="require">*</s>公司传真：</td>
                        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="正常状态"></td>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <td class="txt_r"><s class="require">*</s>纳税人识别号：</td>
                        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="正常状态"></td>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <td class="txt_r"><s class="require">*</s>开户银行：</td>
                        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="正常状态"></td>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <td class="txt_r"><s class="require">*</s>开户银行账号：</td>
                        <td><input type="text" class="iIpt iIpt_sm" value="" placeholder="正常状态"></td>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <td class="txt_r"><s class="require">*</s>发票类型：</td>
                        <td>
                            <select name="reasonId" id="reasonId" class="ipt inp-sel">
                                <option value="--">选择发票类型</option>
                                <option value="555">增值税专用发票</option>
                                <option value="666">企业</option>
                                <option value="777">个人</option>
                            </select>
                        </td>
                        <td>&nbsp;</td>
                    </tr>
                    </tbody>
                </table>

            </div>
        </div>

        <!-- 上传开票附件 板块-->
        <div id="upInvoicePlate" class="plateBox">
            <div class="plateHead .borSolid">
                <!--<span class="prefix"></span>-->
                <h3 class="headLeft fl"><s class="require">*</s> 上传开票信息</h3>
                <p class="fl">( 请先下载样板，然后拍照上传 )</p>
                <!--<p class="headContent">居中标题</p>-->
                <!--<p class="headRight">-->
                    <!--<span class="doubtInlet">-->
                        <!--<s class="require">*</s> 为必填项目-->
                    <!--</span>-->
                <!--</p>-->
            </div>
            <div class="plateMain clearfix">
                <div class="fl">
                    <img src="/dist/images/compact/invoiceTemplate.jpg" alt=""/>
                    <p>客户开票信息表</p>
                    <a href="" class="iBtn iBtn_down">
                        <i class="iBtn_down_icn"></i>下载附件
                    </a>

                </div>
                <div class="fr">


                </div>
            </div>

            <!-- 操作按钮-->
            <div class="plateMain txt_c">
                <br/><br/>
                <input type="button" class="iBtn iBtn_def" value="取消">
                <input type="button" class="iBtn iBtn_yes ml20" value="确认">
            </div>

        </div>


    </div>
</div>






<%- include ../global/footer/footer %>
<%- include ../global/includehead/bottomscript %>
<script src="/dist/scripts/settlement/confirmTheInvoice.js"></script>

</body>
</html>



