        /* 备注.开票信息 页面 --------------------------- */
        @keyframes spin { from { transform: scale(1) rotate(0deg); }
            to { transform: scale(1) rotate(360deg); } }

        @-webkit-keyframes spin2 { from { -webkit-transform: rotate(0deg); }
            to { -webkit-transform: rotate(360deg); } }

        /* 备注.开票信息 页面 --------------------------- */
        #InvoiceInfoBox { width: 460px; padding-top: 16px; padding-bottom: 10px; }

        #InvoiceInfoBox .table tbody > tr { font-size: 14px; }

        #InvoiceInfoBox .table tbody > tr th { line-height: 30px; color: #666666; }

        #InvoiceInfoBox .table tbody > tr td { line-height: 30px; color: #333333; }

        #InvoiceThumbnail { width: 480px; padding-top: 20px; padding-bottom: 20px; }

        #InvoiceThumbnail .invoiceImgTit { line-height: 22px; display: block; padding: 8px 0; }

        #InvoiceThumbnail .thumbnailBox { padding-top: 20px; padding-bottom: 10px; background: #f5f5f5; }

        #InvoiceThumbnail .thumbnailPic { position: relative; width: 380px; height: 262px; margin: 0 auto; text-align: center; cursor: pointer; }

        #InvoiceThumbnail .thumbnailPic .clickLarger { position: absolute; width: 82px; height: 24px; right: 1px; bottom: 1px; z-index: 12; color: #FFFFFF; font-size: 12px; line-height: 24px; text-indent: 20px; background: #333333 url("/static/images/sprite_ico.png") no-repeat -545px -173px; opacity: 0.5; filter: alpha(opacity=50); }

        #InvoiceThumbnail .thumbnailPic img { width: 377px; height: 260px; }

        #InvoiceThumbnail .thumbnailName { height: 40px; padding-top: 8px; font-size: 18px; line-height: 30px; text-align: center; }

        #InvoiceInfoViewPlate{height: 310px;}

        #InvoiceInfoViewPlate.bor_btm { border-bottom: 1px dashed #666666; }
        .bor_btm { border-bottom: 1px dashed #666666; }

        .invoiceInfoPic_md { text-align: center; padding: 20px 20px; }

        .invoiceInfoPic_md img { max-width: 860px; }

        @keyframes spin { from { transform: scale(1) rotate(0deg); }
            to { transform: scale(1) rotate(360deg); } }

        @-webkit-keyframes spin2 { from { -webkit-transform: rotate(0deg); }
            to { -webkit-transform: rotate(360deg); } }

        /* 结算单表格 板块 --------------------------- */
        @keyframes spin { from { transform: scale(1) rotate(0deg); }
            to { transform: scale(1) rotate(360deg); } }

        @-webkit-keyframes spin2 { from { -webkit-transform: rotate(0deg); }
            to { -webkit-transform: rotate(360deg); } }

        .settlementTable { width: 100%; max-width: 100%; }

        .settlementTable tbody tr th, .settlementTable tbody tr td { padding: 9px; font-size: 14px; line-height: 1.428571429; vertical-align: top; border: 1px solid #666666; text-align: center; }

        .settlementTable tbody tr th { background: #f5f5f5; color: #666666; }

        .settlementTable tbody tr td { padding: 15px 0; background: #ffffff; color: #333333; }

        .settlementTable tbody .billBox { position: relative; background: #f3fcff; }

        .settlementTable tbody .billBox .billIpt { width: 90%; border: none; background: #f3fcff; text-align: center; }

        .settlementTable tbody .billBox .errInfo { width: 200px; height: 18px; padding: 0 3px; position: absolute; z-index: 6; font-size: 12px; line-height: 18px; bottom: 0; right: 0; left: 0; margin: 0 auto; color: #ff0000; }

        .settlementTit { height: 50px; line-height: 42px; font-size: 22px; color: #333333; text-align: center; }

        .settlementTit .subEditTitle { font-size: 12px; color: #666666; }

        .settlementTit .subEditTitle .require { line-height: 16px; }

    </style>
</head>
<body>
<!-- 以下.内容部分 -->


<div class="containerWrap">
    <div class="bodyWrap">

        <div id="doubtPlate" class="plateBox">
            <div class="plateMain">
                <!-- 结算单信息 板块-->

                <% var orderObj = data.order %>
                <!-- 结算单信息 板块-->
                <div id="settlementInfoPlate">
                    <!-- 结算信息 -->
                    <div class="mt20">
                        <h2 class="settlementTit">结算单</h2>
                        <table class="settlementTable">
                            <tbody>
                            <tr>
                                <th width="230">订单号</th>
                                <th width="230">合同编号</th>
                                <th>确认提货时间</th>
                                <th width="230">结算时间</th>
                            </tr>
                            <tr>
                                <td><%= orderObj.orderNO %> </td>
                                <td><%= orderObj.contractNO %> </td>
                                <td><%= orderObj.confirmDeliveryTime %></td>
                                <td><%= orderObj.sellerSettleTime %></td>
                            </tr>
                            <tr>
                                <th>港口</th>
                                <th>煤种</th>
                                <th>合同吨位（吨）</th>
                                <th>合同基准单价（元/吨）</th>
                            </tr>
                            <tr>
                                <td><%= orderObj.harbour || '' %> </td>
                                <td><%= orderObj.coalType || '' %> </td>
                                <td><%= orderObj.amount || 0  %></td>
                                <td><%= orderObj.price || 0 %></td>
                            </tr>
                            <tr>
                                <th>合同金额（元）</th>
                                <th>已付款金额（元）</th>
                                <th>付款时间</th>
                                <th>&nbsp;</th>
                            </tr>
                            <tr>
                                <td><%= orderObj.totalMoney || 0 %></td>
                                <td><span class="totalMoney" data-num="<%=orderObj.totalMoney%>"><%= orderObj.totalMoney || 0 %></span> </td>
                                <td><%= orderObj.paymentTime %></td>
                                <td>&nbsp;</td>
                            </tr>
                            </tbody>
                        </table>
                    </div>

                    <!-- 实际结算数据 -->
                    <div class="mt20">
                        <% var isShowEdit_sj = (userType == 'sell' && (orderObj.status == 'WaitSettleAccounts' || orderObj.status == 'ReturnedSettleAccounts')) %>
                        <h2 class="settlementTit">实际结算数据
                            <% if(isShowEdit_sj) {%><span class="subEditTitle">(带 <s class="require">*</s>的为必填项)</span><%}%>
                        </h2>
                        <table class="settlementTable">
                            <tbody>
                            <tr>
                                <th width="230">
                                    <% if(isShowEdit_sj) {%><s class="require">*</s><%}%>
                                    总结算吨位（吨）
                                </th>
                                <th width="230">
                                    <% if(isShowEdit_sj) {%><s class="require">*</s><%}%>
                                    港务费（元）
                                </th>
                                <th>
                                    <% if(isShowEdit_sj) {%><s class="require">*</s><%}%>
                                    总结算金额（元）
                                </th>
                                <th width="230">
                                    <% if(isShowEdit_sj) {%><s class="require">*</s><%}%>
                                    <span class="refundMoneyTit">
                                        <% if(orderObj.tailMoney > orderObj.refundMoney){ %>
                                        应补货款金额（元）
                                        <% }else{ %>
                                        应退货款金额（元）
                                        <% } %>
                                    </span>
                                </th>
                            </tr>

                            <% if(isShowEdit_sj) {%>
                            <tr>
                                <td class="billBox">
                                    <input type="text" class="billIpt" name="ins_ettleAmount" maxlength="9" placeholder="请输入大于0的数值" value="<%= formatDecimal((orderObj.settleAmount || ''), 2) %>" />
                                    <span class="errInfo"></span>
                                </td>
                                <td class="billBox">
                                    <input type="text" class="billIpt" name="ins_harbourDues" maxlength="11" placeholder="请输入大于0的数值" value="<%= formatDecimal((orderObj.harbourDues || ''), 2) %>"/>
                                    <span class="errInfo"></span>
                                </td>
                                <td class="billBox">
                                    <input type="text" class="billIpt" name="ins_settleMoney" maxlength="11" placeholder="请输入大于0的数值" value="<%= formatDecimal((orderObj.settleMoney || ''), 2) %>"/>
                                    <span class="errInfo"></span>
                                </td>
                                <td>
                                    <% if(orderObj.tailMoney > orderObj.refundMoney){ %>
                                    <span class="refundMoney" data-num="<%= orderObj.tailMoney || 0 %>"><%= orderObj.tailMoney || 0 %></span>
                                    <% }else{ %>
                                    <span class="refundMoney" data-num="<%= orderObj.refundMoney || 0 %>"><%= orderObj.refundMoney || 0 %></span>
                                    <% } %>
                                </td>
                            </tr>
                            <tr>
                                <th>说明</th>
                                <td colspan="3" class="billBox">
                                    <textarea class="txtArea txtRemarks" name="ins_remarks" rows="1" maxlength="500" placeholder="请填写关于结算的说明，如没有说明即可不用填写。"><%= orderObj.remarks || '' %></textarea>
                                </td>
                            </tr>


                            <% } else { %>
                            <tr>
                                <td><%= orderObj.settleAmount || 0 %></td>
                                <td><%= orderObj.harbourDues || 0 %></td>
                                <td><%= orderObj.settleMoney || 0 %></td>
                                <td>
                                    <% if(orderObj.tailMoney > orderObj.refundMoney){ %>
                                    <span class="refundMoney"><%= orderObj.tailMoney || 0 %></span>
                                    <% }else{ %>
                                    <span class="refundMoney"><%= orderObj.refundMoney || 0 %></span>
                                    <% } %>
                                </td>
                            </tr>
                            <tr>
                                <th>说明</th>
                                <td colspan="3"><%= orderObj.remarks %></td>
                            </tr>
                            <% } %>


                            <tr>
                                <th colspan="2">购方公司</th>
                                <th colspan="2">销方公司</th>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <span><%= orderObj.buyerCompanyName %></span>
                                </td>
                                <td colspan="2">
                                    <span><%= orderObj.sellerCompanyName %></span>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>











            </div>
        </div>
        <p class="mt20 bor_btm"></p>

        <div id="InvoiceInfoViewPlate" class="plateBox bor_btm">
            <div class="plateHead pad-t20">
                <!--<span class="prefix"></span>-->
                <%  var subTitle = subTitle || '查看开票信息' %>
                <h3 class="headLeft"><%= subTitle %></h3>
            </div>
            <div class="plateMain clearfix">
                <div class="fl" id="InvoiceInfoBox">
                    <table class="table tab_nBor">
                        <tbody>
                        <tr>
                            <th class="txt_r" width="130">公司名称：</th>
                            <td width="340">
                                <span class="a"><%=data.receipt && data.receipt.companyName? data.receipt.companyName : ""%></span>
                            </td>
                        </tr>
                        <tr>
                            <th class="txt_r">公司地址：</th>
                            <td><span class="a"><%=data.receipt && data.receipt.companyAddress? data.receipt.companyAddress : ""%></span></td>
                        </tr>
                        <tr>
                            <th class="txt_r">公司电话：</th>
                            <td><span class="a"><%=data.receipt && data.receipt.companyPhone? data.receipt.companyPhone : ""%></span></td>
                        </tr>
                        <tr>
                            <th class="txt_r">公司传真：</th>
                            <td><span class="a"><%=data.receipt && data.receipt.companyFax? data.receipt.companyFax : ""%></span></td>
                        </tr>
                        <tr>
                            <th class="txt_r">纳税人识别号：</th>
                            <td><span class="a"><%=data.receipt && data.receipt.identificationNumber? data.receipt.identificationNumber : ""%></span></td>
                        </tr>
                        <tr>
                            <th class="txt_r">开户银行：</th>
                            <td><span class="a"><%=data.receipt && data.receipt.bankName? data.receipt.bankName : ""%></span></td>
                        </tr>
                        <tr>
                            <th class="txt_r">开户银行账号：</th>
                            <td><span class="a"><%=data.receipt && data.receipt.bankAccount? data.receipt.bankAccount : ""%></span></td>
                        </tr>
                        <tr>
                            <th class="txt_r">发票类型：</th>
                            <td><span class="a"><%=data.receipt && data.receipt.typeName? data.receipt.typeName : ""%></span></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- 发票备注 板块-->
        <div id="InvoiceRemarksPlate" class="plateBox mt20">
            <form action="" method="post">
                <!-- 发票备注-->
                <div class="plateHead">
                    <h3 class="headLeft"> 对“发票备注”内容是否有明确的要求？</h3>
                </div>
                <div class="plateMain">
                    <div class="txtAreaBox">
                        <% if(data.orderReceiptRemarks != null){ %>
                        <textarea class="disabled_textarea" id="requirement" name="requirement" disabled><%= data.orderReceiptRemarks.requirement %></textarea>
                        <%}else{%>
                        <textarea class="disabled_textarea" id="requirement" name="requirement" disabled>无</textarea>
                        <%}%>
                    </div>
                </div>

                <!-- 特殊备注-->
                <div class="plateHead mt20">
                    <h3 class="headLeft fl"> 其他特殊要求</h3>
                </div>
                <div class="plateMain">
                    <div class="txtAreaBox">
                        <% if(data.orderReceiptRemarks != null){ %>
                        <textarea class="disabled_textarea" id="specialRequirement" name="specialRequirement" disabled><%= data.orderReceiptRemarks.specialRequirement%></textarea>
                        <%}else{%>
                        <textarea class="disabled_textarea" id="specialRequirement" name="specialRequirement" disabled>无</textarea>
                        <%}%>
                    </div>
                </div>

                <!-- 操作按钮-->
                <% if(editable !="false" ){ %>

                <div class="plateMain txt_c">
                    <br/><br/>
                    <input type="button" id="cancelBtn" class="iBtn iBtn_def" value="取消">
                    <input type="button" id="submitInvoiceNoteBtn" class="iBtn iBtn_yes ml20" value="确认">
                </div>

                <%}else{%>

                <%}%>
            </form>
        </div>

    </div>
</div>



</body>
</html>
