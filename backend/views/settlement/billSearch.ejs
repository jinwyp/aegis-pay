<form class="billSearch" id="billSearch"  action="/settlement/billCenter">
    <span>结算时间&nbsp;:&nbsp;&nbsp;</span>
    <div class="time"><input type="text" name="startDate" class="iIpt iIpt_sm startDate" placeholder="yyyy-mm-dd" value="<%=receiptOrder.startDate%>"/></div>
    <span class="to">&nbsp;&nbsp;至&nbsp;&nbsp;</span>
    <div class="time"><input type="text" name="endDate" class="iIpt iIpt_sm endDate" placeholder="yyyy-mm-dd" value="<%=receiptOrder.endDate%>"/></div>
    <span class="bill">订单号&nbsp;:&nbsp;&nbsp;</span>
    <input type="text" name="content" class="iIpt iIpt_sm record-number billNumber" id="billNumber" placeholder="请输入订单编号" value="<%=receiptOrder.content%>"/>
    <input type="hidden" name="type" value="<%=receiptOrder.type%>"/>
    <input type="submit" class="iBtn iBtn_sm iBtn_yes ml-40" value="查询" id="submit">
</form>