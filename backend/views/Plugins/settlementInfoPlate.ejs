<!-- 结算单信息 板块-->
<div id="settlementInfoPlate">
    <!-- 结算信息 -->
    <div class="mt20">
        <h2 class="settlementTit">结算单</h2>
        <table class="settlementTable">
            <tbody>
            <tr>
                <th width="230">订单号</th>
                <th width="230">合同编号</th>
                <th>确认提货时间</th>
                <th width="230">结算时间</th>
            </tr>
            <tr>
                <td>82793847398 </td>
                <td>82793847398 </td>
                <td>2016-05-31 16:29:18</td>
                <td>2016-05-31 16:29:22</td>
            </tr>
            <tr>
                <th>港口</th>
                <th>煤种</th>
                <th>合同吨位（吨）</th>
                <th>合同基准单价（元/吨）</th>
            </tr>
            <tr>
                <td>无烟煤 </td>
                <td>4000 </td>
                <td>400</td>
                <td>40,000.00</td>
            </tr>
            <tr>
                <th>合同金额（元）</th>
                <th>已付款金额（元）</th>
                <th>付款时间</th>
                <th>&nbsp;</th>
            </tr>
            <tr>
                <td>40,000.00</td>
                <td>40,000.00 </td>
                <td>400</td>
                <td>&nbsp;</td>
            </tr>
            </tbody>
        </table>
    </div>
    <!-- 实际结算数据 -->
    <div class="mt20">
        <% var isShowEdit_sj = (userType == 'sell' && (order.status == 'WaitSettleAccounts' || order.status == 'ReturnedSettleAccounts')) %>
        <h2 class="settlementTit">实际结算数据
            <% if(isShowEdit_sj) {%><span class="subEditTitle">(带 <s class="require">*</s>的为必填项)</span><%}%>
        </h2>
        <table class="settlementTable">
            <tbody>
            <tr>
                <th width="230">
                    <% if(isShowEdit_sj) {%><s class="require">*</s><%}%>
                    总结算吨位（吨）
                </th>
                <th width="230">
                    <% if(isShowEdit_sj) {%><s class="require">*</s><%}%>
                    港务费（元）
                </th>
                <th>
                    <% if(isShowEdit_sj) {%><s class="require">*</s><%}%>
                    总结算金额（元）
                </th>
                <th width="230">
                    <% if(isShowEdit_sj) {%><s class="require">*</s><%}%>
                    <span class="giveChangeTit">应补货款金额（元）</span>
                </th>
            </tr>

            <% if(isShowEdit_sj) {%>
            <tr>
                <td class="billBox">
                    <input type="text" class="billIpt" placeholder="0-100整数"/>
                    <span class="errInfo"></span>
                </td>
                <td class="billBox">
                    <input type="text" class="billIpt" placeholder="0-100整数"/>
                    <span class="errInfo"></span>
                </td>
                <td class="billBox">
                    <input type="text" class="billIpt" placeholder="0-100整数"/>
                    <span class="errInfo"></span>
                </td>
                <td><span class="giveChangeNum">880.00</span></td>
            </tr>
            <tr>
                <th>说明</th>
                <td colspan="3" class="billBox">
                    <input type="text" class="billIpt" placeholder="请填写关于结算的说明，如没有说明即可不用填写"/>
                    <span class="errInfo"></span>
                </td>
            </tr>

            <% } else { %>
            <tr>
                <td>3234623</td>
                <td>23451236</td>
                <td>1232446457</td>
                <td><span class="giveChangeNum">880.00</span></td>
            </tr>
            <tr>
                <th>说明</th>
                <td colspan="3">243125135</td>
            </tr>
            <% } %>

            <tr>
                <th colspan="2">购方公司</th>
                <th colspan="2">销方公司</th>
            </tr>
            <tr>
                <td colspan="2">
                    <span>买家公司名称全称</span>
                </td>
                <td colspan="2">
                    <span>卖家公司名称全称</span>
                </td>
            </tr>
            </tbody>
        </table>
    </div>
</div>